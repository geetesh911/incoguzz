{
  "name": "@incoguzz/server",
  "version": "0.0.0",
  "description": "TypeScript + Mongoose + MongoDB + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "dev:test": "dotenv -e .env.test -- yarn dev",
    "build": "tsc && npx tsc-alias",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "lint:fix": "yarn lint -- --fix",
    "deploy:prod": "yarn build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "prisma:update": "yarn add -D typegraphql-prisma@latest prisma@latest && yarn add @prisma/client@latest",
    "prisma:validate": "prisma validate --schema=./src/prisma/schema.prisma",
    "prisma:generate": "yarn prisma:validate && prisma generate --schema=./src/prisma/schema.prisma && yarn pretty",
    "prisma:dbpush": "yarn prisma:validate && prisma db push --schema=./src/prisma/schema.prisma && yarn pretty",
    "prisma:migrate-dev": "yarn prisma:validate && yarn prisma migrate dev --schema=./src/prisma/schema.prisma && yarn pretty",
    "prisma:migrate-dev-createonly": "yarn prisma:validate && yarn yprisma migrate dev --create-only --schema=./src/prisma/schema.prisma && yarn pretty",
    "prisma:migrate-deploy": "yarn prisma:validate && yarn prisma migrate deploy --schema=./src/prisma/schema.prisma && yarn pretty",
    "prisma:seed": "yarn prisma:validate && prisma db seed",
    "prisma:studio": "yarn prisma:validate && prisma studio --schema=./src/prisma/schema.prisma",
    "generate": "graphql-codegen",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "dotenv -e .env.test -- npx prisma db push --schema=./src/prisma/schema.prisma --skip-generate && dotenv -e .env.test -- jest -i"
  },
  "prisma": {
    "seed": "ts-node ./src/databases/seed.ts"
  },
